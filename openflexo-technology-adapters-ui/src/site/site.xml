<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

	<bannerLeft>
		<name>Technology Adapters</name>
		<src>${ta.logo}</src>
		<href>https://openflexo.org/tiki/TechnologyAdapters</href>
	</bannerLeft>
	<bannerRight>
		<name>Openflexo</name>
		<src>${organization.logo}</src>
		<href>http://openflexo.org/</href>
	</bannerRight>
	<skin>
		<groupId>org.openflexo</groupId>
		<artifactId>openflexo-skin</artifactId>
		<version>1.3-SNAPSHOT</version>
	</skin>

	<custom>
		<openflexo-skin>
			<sideMenus>true</sideMenus>
			<displayBreadCrumbs>true</displayBreadCrumbs>
			<localMenu>/inc/html/developersMenu.html</localMenu>
			<includeVersionLinks>true</includeVersionLinks>
		</openflexo-skin>
	</custom>

	<body>

		<menu name="Technology-Adapters UI layer">
			<item name="Home" href="index.html" />
				<item name="Diagram-UI" href="flexodiagram-ui/index.html" />
				<item name="OWL-UI" href="owlconnector-ui/index.html" />
				<item name="EMF-UI" href="emfconnector-ui/index.html" />
				<item name="XML/XSD-UI" href="xmlconnector-ui/index.html" />
				<item name="FreePlane-UI" href="freeplaneconnector-ui/index.html" />
				<item name="Word-UI" href="docxconnector-ui/index.html" />
				<item name="Excel-UI" href="excelconnector-ui/index.html" />
				<item name="PowerPoint-UI" href="powerpointconnector-ui/index.html" />
				<item name="OSLC-UI" href="oslcconnector-ui/index.html" />
		</menu>

        <menu ref="parent" />
		<menu ref="modules" />
		<menu ref="reports" />
	</body>
</project>
